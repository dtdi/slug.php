{
  "name": "dtdi/slug.php",
  "description": "A little helper API that generates URL-safe Slugs for URLs.",
  "type": "project",
  "keywords": [
    "helper",
    "url generation"
  ],
  "require": {
    "illuminate/http": "^10.35",
    "illuminate/support": "^10.35",
    "sqids/sqids": "^0.4.1",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "authors": [
    {
      "name": "Tobias Fehrer",
      "email": "tobias@dtdi.de"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
